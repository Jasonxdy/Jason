package com.kh.chap03_throws.controller;

public class ThrowsController {
	
	public void method1 () {
		System.out.println("method1() 호출");
		method2();
	}
	
	public void method2 () {
		System.out.println("method2() 호출");
		method3();
	}
	
	public void method3 () {
		System.out.println("method3() 호출");
	}
	
	public void method4 () {
		System.out.println("method4() 호출");
	}
	
	
	

}
